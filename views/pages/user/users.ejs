<!-- views/pages/users.ejs -->
<!-- The users coupons page for coupons will have search/list/add/delete/update UI here -->
<!DOCTYPE html>
<html lang="en">

<!-- User head in partials/head.ejs -->
<head>
  <% include ../../partials/common/head %>
  <% include ../../partials/user/head %>
</head>

<body>

  <!-- User header in partials/common/header.ejs -->
  <header>
    <% include ../../partials/common/header %>
  </header>

  <!-- Main body -->
  <main  class="container">
    <div class="jumbotron userTitle">
      <h1>Users</h1>
      <h4>You can search all coupons for one user</h4>

      <div class="row">
        <div class="col-lg-8">
          <div class="input-group">
            <span class="input-group-btn">
              <button id="userSearch" class="btn btn-warning" type="button">
                <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                Search</button>
              </span>
              <input id="userSearchInput" type="text" class="form-control" placeholder="Input username...">
            </div>
          </div>
        </div>
      </div>

      <!-- Show CouponList in partials/coupon/couponList.ejs -->
      <div>
        <% include ../../partials/coupon/couponList %>
      </div>

    </main>


    <!-- User footer in partials/common/footer.ejs -->
    <footer>
      <% include ../../partials/common/footer %>
    </footer>

  </body>
  </html>
